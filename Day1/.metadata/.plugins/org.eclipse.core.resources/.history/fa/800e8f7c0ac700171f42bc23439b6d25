import java.util.Scanner;

public class Third {

	public static void main(String[] args) {
		int counter = 0;
		Scanner sc = new Scanner(System.in);
		String str = sc.nextLine();
		System.out.println(str);
		sc.close();
		char[] cstr = str.toCharArray();
		for(char i : cstr) {
			if(i == 'x') 
				counter++;
		}
		System.out.println(counter);
	}

}
